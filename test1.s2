import "base.s2"

struct Header
{
	Uint32 magic
}

struct SomeFormat
{
	Header header
	Uint16 something
	Uint16 something2
}

struct Box<$T>
{
	$T something
	Uint8 number
}

struct Pair<$T1, $T2>
{
	$T1 first
	$T2 second
}

struct Boxuse
{
	Box<Uint32> box
	Box<Uint8> box2
	Pair<Header, SomeFormat> weirdPair
	Pair<Header, Pair<Uint32, Uint16>> anotherPair
}

struct Collection<$T>
{
	$T head
	Collection<$T> tail
}

struct Music
{
	Collection< Pair<Uint32, Uint16> > notes
}

[BigEndian]
struct AttributeTest
{
	Uint32 bigEndianNumber

	[LittleEndian]
	Uint32 littleEndianNumber
}

[Attribute]
struct GenericClassWithAttribute<$T>
{
	$T thing
}

struct GenAttributeTest
{
	GenericClassWithAttribute<Uint32> a
}

[Version: major=5, minor=6]
struct AttributeWithExpression
{
	Uint32 filler
}
